#include <iostream>
using namespace std;
int main() {
	char a, b;
	cin>>a>>b;
	if(a=='R' && b=='B')
	cout<<a;
	if(a=='R' && b=='G')
	cout<<a;
	if(a=='G' && b=='B')
	cout<<b;
	if(a=='G' && b=='R')
	cout<<b;
	if(a=='B' && b=='R')
	cout<<b;
	if(a=='B' && b=='G')
	cout<<a;
	if(a==b)
	cout<<a;
	return 0;
}
