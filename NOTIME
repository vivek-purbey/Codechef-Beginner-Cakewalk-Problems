#include<iostream>
using namespace std;
int main() {
	    int n,h,x,m,count=0;
	    cin>>n>>h>>x;
	    for(int i=1;i<=n;i++){
	        cin>>m;
	        if(x+m>=h)
	        count++;
	        else
	        count;
	    }
	    	if(count==0)
	        cout<<"NO"<<endl;
	        else
	        cout<<"YES"<<endl;
	return 0;
}
