#include<iostream>
using namespace std;
int main() {
	int T;
	cin>>T;
	while(T--){
	    int n,m,sum_money=0,min=10001;
	    cin>>n;
	    for(int i=1;i<=n;i++)
	    {
	        cin>>m;
	        if(m<min)
	        min=m;
	        sum_money+=m;
	    }
	    sum_money-=min;
	    cout<<sum_money<<endl;
	}
	return 0;
}
