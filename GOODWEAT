#include <iostream>
using namespace std;
int main() {
	int T;
	cin>>T;
	while(T--){
	    int a1, a2, a3, a4, a5, a6, a7, zero=0, one=0;
	    cin>>a1>>a2>>a3>>a4>>a5>>a6>>a7;
	        if(a1==1)
	        one++;
	        else
	        zero++;
	        if(a2==1)
	        one++;
	        else
	        zero++;
	        if(a3==1)
	        one++;
	        else
	        zero++;
	        if(a4==1)
	        one++;
	        else
	        zero++;
	        if(a5==1)
	        one++;
	        else
	        zero++;
	        if(a6==1)
	        one++;
	        else
	        zero++;
	        if(a7==1)
	        one++;
	        else
	        zero++;
	        if(one>zero)
	        cout<<"YES"<<endl;
	        else
	        cout<<"NO"<<endl;
	}
	return 0;
}
