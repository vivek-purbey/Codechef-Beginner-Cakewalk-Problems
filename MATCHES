#include <iostream>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int a,b,count=0;
	    cin>>a>>b;
	    int sum=a+b;
	    int arr[10]={6,2,5,5,4,5,6,3,7,6};
	    while(sum>=1){
	        int x;
	        x=sum%10;
	        sum=sum/10;
	        count+=arr[x];
	    }
	    cout<<count<<endl;
	}
}
